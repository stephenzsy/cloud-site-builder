{
  "kind": "collectionType",
  "collectionName": "layouts",
  "info": {
    "singularName": "layout",
    "pluralName": "layouts",
    "displayName": "Layout",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "align": {
      "type": "enumeration",
      "enum": [
        "stack-block",
        "stack-inline",
        "grid"
      ],
      "required": true,
      "default": "stack-block"
    },
    "htmlTag": {
      "type": "enumeration",
      "enum": [
        "div",
        "main",
        "header",
        "footer"
      ],
      "default": "div",
      "required": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "block.heading",
        "block.site-logo",
        "block.site-title"
      ]
    }
  }
}
